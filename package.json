{
  "name": "agenda-contatos",
  "homepage": "https://wisnermarques.github.io/agenda-contatos/",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.53.1",
    "@testing-library/dom": "^10.4.1",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "cy:open": "cypress open --e2e",
    "cy:run": "cypress run --e2e",
    "e2e:dev": "start-server-and-test start http://localhost:3000 \"cypress run --e2e\"",
    "e2e:ui": "start-server-and-test start http://localhost:3000 \"cypress open --e2e\""
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/steps/**/*.js",
      "cypress/support/step_definitions/**/*.js"
    ],
    "filterSpecs": true,
    "omitFiltered": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@badeball/cypress-cucumber-preprocessor": "^23.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.5",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^30.0.5",
    "cypress": "^15.1.0",
    "esbuild": "^0.25.9",
    "gh-pages": "^6.3.0",
    "jest": "^30.0.5",
    "start-server-and-test": "^2.0.13"
  }
}